@model IEnumerable<SIPCA.CLASES.Models.TipoEntrega>

@{
    ViewBag.Title = "Tipo de Entrega";
}

<h2>Tipo de Entrega</h2>

<p>
    @Html.ActionLink("Crear Nuevo", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreTipoEntrega)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Costo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreTipoEntrega)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Costo)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.IdTipoEntrega }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.IdTipoEntrega }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.IdTipoEntrega })
        </td>
    </tr>
}

</table>
